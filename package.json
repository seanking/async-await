{
  "name": "es2015-promises",
  "version": "1.0.0",
  "description": "Promises in ES2015",
  "scripts": {
    "lint": "eslint .",
    "test": "mocha --compilers js:babel-register",
    "pretest": "npm run lint"
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "env": {
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-use-before-define": 0
    },
    "ecmaFeatures": {
      "generators": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanking/es2015-promises.git"
  },
  "author": "Sean King",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seanking/es2015-promises/issues"
  },
  "homepage": "https://github.com/seanking/es2015-promises#readme",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "mocha": "^2.4.5",
    "promise": "^7.1.1"
  }
}
